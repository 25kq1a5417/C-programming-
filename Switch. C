#include<stdio.h>
#include<conio.h>
void main ()
{
int a,b,c;
char ch;
clrscr();
printf("Enter ch value ");
scanf("%c",&ch);
printf("Enter a,b value ");
scanf("%d%d",&a,&b);
switch(ch)
{
case'+':
c=a+b;
printf ("%d",c);
break;
case'-':
c=a-b;
printf("%d",c);
break;
case'*':
c=a*b;
printf("%d",c);
break;
case'/':
c=a/b;
printf("%d",c);
break;
}
getch();
}
