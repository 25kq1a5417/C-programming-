#include <stdio.h>

int main() {
    int a, b, c;

    printf("Enter three numbers: ");
    scanf("%d %d %d", &a, &b, &c);

    if (a > b && a > c) {
        printf("%d is the biggest number.\n", a);
    }
    else if (b > a && b > c) {
        printf("%d is the biggest number.\n", b);
    }
    else if (c > a && c > b) {
        printf("%d is the biggest number.\n", c);
    }
    else {
        printf("All numbers are equal or two numbers are equal and biggest.\n");
    }

    return 0;
}
